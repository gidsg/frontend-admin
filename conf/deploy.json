{
    "packages":{
        "frontend-admin":{
            "type":"executable-jar-webapp",
            "apps":[ "frontend::admin" ],
            "data":{
                "port":"9000",
                "bucket":"aws-frontend-artifacts",
                "healthcheck_paths":[
                    "/login"
                ]
            }
        }
    },
    "recipes":{
        "default":{
            "depends" : ["artifactUpload", "deploy"]
        },
        "deploy":{
            "actionsPerHost": ["frontend-admin.deploy"]
        },
        "artifactUpload":{
            "actionsBeforeApp": ["frontend-admin.uploadArtifacts"]
        }
    }
}